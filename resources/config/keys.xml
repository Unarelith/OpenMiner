<!-- Key names are defined here: https://wiki.libsdl.org/SDL_Keycode -->
<keys>
	<Left      key="A"/>
	<Right     key="D"/>
	<Up        key="W"/>
	<Down      key="S"/>

	<Jump      key="Space"/>
	<Fly       key="X"/>
	<Sneak     key="Left Shift"/>
	<Sprint    key="Left Ctrl"/>

	<Dig       key="L"/>
	<Use       key="M"/>

	<Inventory key="E"/>
</keys>
